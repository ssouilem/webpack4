{
  "parser"  : "babel-eslint",
  "extends" : [
    "standard",
    "standard-react"
  ],
  "plugins" : [
    "react",
  ],
  "env"     : {
    "browser" : true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "quotes": [2, "single"],
    "no-alert": 0,
    "react/jsx-curly-spacing": [2, "always"],
    "react/prop-types": [0, {"ignore": ["*"]}],
  }
}
